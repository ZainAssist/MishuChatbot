{
    "name": "Mishu Chat Bot",    
    "env": {
        "BOT_TOKEN": {
            "description": "telegram bot token here",
            "required": true
        },        
        "MONGO_URL": {
            "description": "mongo db url here",
            "required": true
        },
        "GEMINI_API_KEY": {
            "description": "gemini api id here",
            "required": true
        },
        "LOG_CHANNEL_ID": {
            "description": "telegram log channel here",
            "required": true
        }
    },
    "addons": [
        {
            "plan": "heroku-postgresql"
        }
    ]
}
